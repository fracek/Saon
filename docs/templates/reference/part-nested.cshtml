@if (Enumerable.Count(Model.Types) > 0 || Enumerable.Count(Model.Modules) > 0)
{
    <table class="highlight type-list">
        <thead>
            <tr>
                <td>Type or Module</td>
                <td>Description</td>
            </tr>
        </thead>
        <tbody>
        @foreach (var it in Model.Types)
        {
            <tr>
                <td class="type-name">
                    <a href="@(it.UrlName).html">@it.Name</a>
                </td>
                <td class="xmldoc">
                    @if (it.IsObsolete)
                    {
                        <div class="alert alert-warning">
                            <strong>WARNING: </strong> This API is obsolete
                            <p>@Html.Encode(@it.ObsoleteMessage)</p>
                        </div>
                    }
                    @it.Comment.Blurb
                </td>
            </tr>
        }
        @foreach (var it in Model.Modules)
        {
            <tr>
                <td class="module-name">
                    <a href="@(it.UrlName).html">@it.Name</a>
                </td>
                <td class="xmldoc">
                    @if (it.IsObsolete)
                    {
                        <div class="alert alert-warning">
                            <strong>WARNING: </strong> This API is obsolete
                            <p>@Html.Encode(@it.ObsoleteMessage)</p>
                        </div>
                    }
                    @it.Comment.Blurb
                </td>
            </tr>
        }
        </tbody>
    </table>
}
